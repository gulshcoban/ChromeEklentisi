{
	"name":"ChromeSudoku",
	"description":"Sudoku Oyun Eklentisi",
	"version":"0.0.0.1",
	"manifest_version":2,
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"browser_action": 
	{
	"default_icon":"icon.png",
	"default_popup":"Main.html"
 	},
	"permissions": [
      "tabs", "http://*/*", "https://*/*"
    ]
}
